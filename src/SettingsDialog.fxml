<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SettingsDialogController">
   <children>
      <GridPane hgap="5.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <GridPane hgap="5.0" vgap="5.0" GridPane.rowIndex="2">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="NEVER" />
                  <ColumnConstraints hgrow="NEVER" />
                  <ColumnConstraints hgrow="NEVER" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="ok" minWidth="65.0" mnemonicParsing="false" text="OK" GridPane.columnIndex="1" />
                  <Button fx:id="cancel" minWidth="65.0" mnemonicParsing="false" text="Cancel" GridPane.columnIndex="2" />
                  <Button fx:id="apply" minWidth="65.0" mnemonicParsing="false" text="Apply" GridPane.columnIndex="3" />
                  <Button fx:id="defaultButton" mnemonicParsing="false" text="default" />
               </children>
            </GridPane>
            <Button fx:id="openAppFolder" mnemonicParsing="false" text="Open app folder" GridPane.rowIndex="1" />
            <GridPane hgap="5.0" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="NEVER" />
                <ColumnConstraints hgrow="SOMETIMES" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <ComboBox fx:id="style" GridPane.columnIndex="1" />
                  <Label text="Style:" GridPane.halignment="RIGHT" />
                  <Label text="&quot;Open in folder&quot; command:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                  <TextField fx:id="openInFolderCommand" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="&quot;Open in folder&quot; argument:" GridPane.rowIndex="2" />
                  <TextField fx:id="openInFolderArgument" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </children>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
